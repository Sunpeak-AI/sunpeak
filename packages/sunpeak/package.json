{
  "name": "sunpeak",
  "version": "0.13.6",
  "description": "The ChatGPT App framework. Quickstart, build, & test your ChatGPT App locally!",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./chatgpt": {
      "import": {
        "types": "./dist/chatgpt/index.d.ts",
        "default": "./dist/chatgpt/index.js"
      }
    },
    "./style.css": "./dist/style.css",
    "./chatgpt/globals.css": "./dist/chatgpt/globals.css",
    "./mcp": {
      "import": {
        "types": "./dist/mcp/index.d.ts",
        "default": "./dist/mcp/index.js"
      }
    },
    "./platform": {
      "import": {
        "types": "./dist/platform/index.d.ts",
        "default": "./dist/platform/index.js"
      }
    },
    "./platform/chatgpt": {
      "import": {
        "types": "./dist/platform/chatgpt/index.d.ts",
        "default": "./dist/platform/chatgpt/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "sunpeak": "./bin/sunpeak.js"
  },
  "files": [
    "dist",
    "bin",
    "template",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "pnpm -C template dev",
    "format": "prettier --write --list-different \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "validate": "node scripts/validate.mjs",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "openai-apps-sdk-ui",
    "openai-app",
    "chatgpt-apps-sdk",
    "chatgpt-app",
    "mcp-ui",
    "mcp",
    "chatgpt-simulator",
    "testing",
    "multi-platform",
    "react",
    "tailwind",
    "sunpeak"
  ],
  "author": "Sunpeak AI",
  "license": "MIT",
  "peerDependencies": {
    "@openai/apps-sdk-ui": "^0.2.1",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/ext-apps": "^1.0.1",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.4.0",
    "esbuild": "^0.27.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.19.6",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.4.0",
    "prettier": "^3.7.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.17"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sunpeak-AI/sunpeak"
  },
  "bugs": {
    "url": "https://github.com/Sunpeak-AI/sunpeak/issues"
  },
  "homepage": "https://sunpeak.ai/"
}

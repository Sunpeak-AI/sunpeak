{
  "name": "sunpeak",
  "version": "0.4.2",
  "description": "The MCP App SDK. Quickstart, build, & test your ChatGPT App locally!",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./style.css": "./dist/style.css",
    "./mcp": {
      "import": {
        "types": "./dist/mcp/index.d.ts",
        "default": "./dist/mcp/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "sunpeak": "./bin/sunpeak.js"
  },
  "files": [
    "dist",
    "bin",
    "template",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "pnpm --filter my-sunpeak-app dev",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "validate": "node scripts/validate.mjs",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "openai-apps-sdk-ui",
    "openai-app",
    "chatgpt-apps-sdk",
    "chatgpt-app",
    "mcp-ui",
    "mcp",
    "chatgpt-simulator",
    "testing",
    "multi-platform",
    "react",
    "tailwind",
    "sunpeak"
  ],
  "author": "Sunpeak AI",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@openai/apps-sdk-ui": "^0.2.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/vite": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.2.0",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sunpeak-AI/sunpeak"
  },
  "bugs": {
    "url": "https://github.com/Sunpeak-AI/sunpeak/issues"
  },
  "homepage": "https://sunpeak.ai/"
}

{
  "name": "sunpeak",
  "version": "0.2.4",
  "description": "The ChatGPT Apps UI SDK. Build and test your ChatGPT App UI locally with approved shadcn React components.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles/globals.css": "./dist/styles/globals.css",
    "./styles/chatgpt": "./dist/styles/chatgpt/index.css",
    "./package.json": "./package.json"
  },
  "bin": {
    "sunpeak": "./bin/sunpeak.js"
  },
  "files": [
    "dist",
    "bin",
    "template",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "pnpm --filter my-sunpeak-app dev",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "openai-app",
    "chatgpt-app",
    "chatgpt-simulator",
    "testing",
    "multi-platform",
    "react",
    "components",
    "shadcn-ui",
    "tailwindcss",
    "sunpeak"
  ],
  "author": "Sunpeak AI",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.554.0",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.2.0",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3",
    "vite": "^5.4.21",
    "vitest": "^4.0.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sunpeak-AI/sunpeak"
  },
  "bugs": {
    "url": "https://github.com/Sunpeak-AI/sunpeak/issues"
  },
  "homepage": "https://sunpeak.ai/"
}
